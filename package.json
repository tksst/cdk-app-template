    "@tksst/prettier-config": "1.1.0",
    "@tksst/typescript-starter-configs": "1.0.0",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.10",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "aws-cdk": "2.53.0",
    "better-typescript-lib": "2.1.0",
    "eslint": "8.28.0",
    "eslint-plugin-jest": "27.1.6",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "29.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "secretlint": "5.3.0",
    "tsx": "3.12.1",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.53.0",
    "constructs": "10.1.176",
    "source-map-support": "0.5.21"
  },
  "packageManager": "pnpm@7.17.1"
